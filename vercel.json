{
    "builds": [{
        "src": "seed/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "seed/wsgi.py"
        }
    ],
    "functions": {
        
        "react/(.*)": {
          "maxDuration": 60 // These functions can run for a maximum of 15 seconds
        }
      }
}
